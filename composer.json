{
  "name": "katalystsol/php-soap-client",
  "type": "library",
  "description": "A generic PHP SOAP Client",
  "keywords": [
    "katalystsol",
    "php-soap-client",
    "soap",
    "soap-client"
  ],
  "homepage": "https://github.com/katalystsol/php-soap-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Don Cranford",
      "email": "dcranford@katalystsolutions.com",
      "homepage": "https://www.katalystsolutions.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "~5.6|~7.0",
    "guzzlehttp/guzzle": "~6.0",
    "ext-json": "*",
    "ext-soap": "*"
  },
  "require-dev": {
    "phpunit/phpunit" : ">=5.4.3",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Katalystsol\\PhpSoapClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Katalystsol\\PhpSoapClient\\Test": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}